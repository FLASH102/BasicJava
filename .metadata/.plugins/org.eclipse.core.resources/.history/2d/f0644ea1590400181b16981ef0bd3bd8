package basicjava;

public final class Student {

	String name;

	Student(String name) {
		this.name = name;

	}

	Student() {

	}

	int play(int gameType) {
		if (gameType == 1) {
			System.out.println("I am playing basketball.");
		} else if (gameType == 2) {
			System.out.println("I am playing soccer.");
		} else {
			System.out.println("I do not know what I am playing.");
		}

		return 10;
	}

	int play2(int gametype) {
		switch  (gametype) {
		case 1:
			System.out.println("I am playing basketball.");
			break;

		case 2:
			System.out.println("I am playing soccer.");
			break;
		default:
			System.out.println("I don't know what I am playing.");
		}
		return 10;
	}
}
