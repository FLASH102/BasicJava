
public class Car {
	String name;
	int power;
boolean hasStarted = false;
	void start() {
	if(hasStarted = false){
		System.out.println("Error--you have already started the " + name);
	}else
		System.out.println("Starting the " + name);
	}boolean hasStarted = true;

	void stop() {
		System.out.println("Stopping the " + name);
	}
}