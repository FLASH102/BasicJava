
public class Math {

	static int addNumbers(int num1, int num2) {
		int z = num1 + num2;
		return z;
	}

	static int findAbsoluteValue(int num1) {
		int result;
		
		if (num1 > 0) {
			result = num1 * 1;
		} else {
			result = num1 * -1;
		}
		
		return result;
	}
}

int addSpecial(int num1) {
	return 0;
	
}